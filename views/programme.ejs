  .navbar {
    background-color: #385c90;
    padding: 15px 20px;
    display: flex;
    justify-content: flex-end; /* جعل العناصر في الجانب الأيمن */
    align-items: center;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
